<?xml version="1.0" encoding="utf-8"?>
<s:View xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:s="library://ns.adobe.com/flex/spark"
		color="#FF0000" overlayControls="true" tabBarVisible="true"
		title="OC TRANSPO +">
	

	
	<fx:Declarations>
	</fx:Declarations>
	<s:List id="list" left="0" right="0" top="90" bottom="0" color="#000000"
			dataProvider="{home_data}" labelField="lastName">
		
		<s:itemRenderer>
			<fx:Component>
				<s:IconItemRenderer
					
					label="{data.title}"
					messageField="label"
				
				/>
			</fx:Component>
		</s:itemRenderer>
			
	</s:List>

<fx:Script>
	<![CDATA[
		import mx.collections.ArrayCollection;
		[Bindable]
		public var home_data:ArrayCollection = new ArrayCollection([
			{title: "Bus Times" , label :"Find Your Bus Times"},
			{title: "Nearby Busses" , label :"Find a Bus Nearby"},
			{title: "Favorites" , label :"Your most frequent busses"},
			
			]);
	]]>
</fx:Script>

</s:View>
